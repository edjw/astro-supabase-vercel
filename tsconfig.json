{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "jsxImportSource": "react"
  }
}